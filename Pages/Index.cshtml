@page
@model IndexModel
@{
    ViewData["Title"] = "Home page";
}
<html lang="en">
<head>

    <title>Cleaning Service Website </title>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=Edge">
    <meta name="description" content="">
    <meta name="keywords" content="">
    <meta name="author" content="Tooplate">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">


    <link rel="stylesheet" href="~/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/css/animate.css">
    <link rel="stylesheet" href="~/css/owl.carousel.css">
    <link rel="stylesheet" href="~/css/owl.theme.default.min.css">

    <!-- MAIN CSS -->

    <link rel="stylesheet" href="~/css/tooplate-style.css">

</head>

<body id="top" data-spy="scroll" data-target=".navbar-collapse" data-offset="50">

    <!-- PRE LOADER -->
    <section class="preloader">
        <div class="spinner">

            <span class="spinner-rotate"></span>

        </div>
    </section>

    <!-- HOME -->
    <section id="home" class="slider" data-stellar-background-ratio="0.5">
        <br />
        <br />
        <div class="container">
            <div class="row">

                <div class="owl-carousel owl-theme">
                    <div class="item item-first">
                        <div class="caption">
                            <div class="col-md-offset-1 col-md-10">
                                <h3>Good Housekeeping</h3>
                                <h1>Is The Key To Safety</h1>
                            </div>
                        </div>
                    </div>

                    <div class="item item-second">
                        <div class="caption">
                            <div class="col-md-offset-1 col-md-10">
                                <h3>A Clean Work Area</h3>
                                <h1>Is A Safe Work Area</h1>
                            </div>
                        </div>
                    </div>

                    <div class="item item-third">
                        <div class="caption">
                            <div class="col-md-offset-1 col-md-10">
                                <h3></h3>
                                <h1>Star Cleaning Service</h1>
                            </div>
                        </div>
                    </div>
                </div>

            </div>
        </div>
    </section>

    <!-- ABOUT -->
    <section id="about">
        <div class="container">
            <div class="row">

                <div class="col-md-6 col-sm-6">
                    <div class="about-info">
                        <h2 class="wow fadeInUp" data-wow-delay="0.6s">Welcome to <i class="fa fa-h-square"></i> Star Cleaning Service </h2>
                        <div class="wow fadeInUp" data-wow-delay="0.8s">
                            <p style="color:black;">Star Cleaning Service is a service for people willing to find housekeeper for their home or office.</p>
                        </div>
                    </div>
                </div>

            </div>
        </div>
    </section>

    <!-- SERVICES -->
    <section id="services" data-stellar-background-ratio="1">
        <h2 class="wow fadeInUp" data-wow-delay="0.1s">Services</h2>
        <div class="container">
            <div class="row">
                <div class="col-md-4 col-sm-4">
                    <div class="team-thumb wow fadeInUp" data-wow-delay="0.2s">
                        <img src="~/images/service-image1.jpg" class="img-responsive" alt="" style="width:310px;height:300px;">
                        <div class="team-info">
                            <h3>Cleaning Houses</h3>
                        </div>
                    </div>
                </div>
                <div class="col-md-4 col-sm-4">
                    <div class="team-thumb wow fadeInUp" data-wow-delay="0.4s">
                        <img src="~/images/service-image2.jpg" class="img-responsive" alt="" style="width:310px;height:300px;">
                        <div class="team-info">
                            <h3>Cleaning Offices</h3>
                        </div>
                    </div>
                </div>
                <div class="col-md-4 col-sm-4">
                    <div class="team-thumb wow fadeInUp" data-wow-delay="0.4s">
                        <img src="~/images/service-image3.jpg" class="img-responsive" alt="" style="width:310px;height:300px;">
                        <div class="team-info">
                            <h3>Cleaning Categories</h3>
                            <ul class="social-icon">
                                <li><h3>Floor Cleaning</h3></li>
                                <li><h3>Window Cleaning</h3></li>
                                <li><h3>Full Cleaning</h3></li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <!-- GOOGLE MAP -->
    <section id="google-map">
        <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d12743709.793692345!2d26.175838542388647!3d38.75780278076299!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x14b0155c964f2671%3A0x40d9dbd42a625f2a!2zVMO8cmtpeWU!5e0!3m2!1str!2str!4v1620746391915!5m2!1str!2str" width="100%" height="350" frameborder="0" style="border:0" allowfullscreen></iframe>
    </section>

    <!-- FOOTER -->
    <footer data-stellar-background-ratio="5">
        <div class="container">
            <div class="row">

                <div class="col-md-4 col-sm-4">
                    <div class="footer-thumb">
                        <h4 class="wow fadeInUp" data-wow-delay="0.4s">Contact Info</h4>
                        <div class="contact-info">
                            <p><i class="fa fa-phone"></i> 010-060-0160</p>
                            <p><i class="fa fa-envelope-o"></i> <a href="#">info@starcleaning.com</a></p>
                        </div>
                    </div>
                </div>

                <div class="col-md-12 col-sm-12 border-top">
                    <div class="col-md-4 col-sm-6">
                        <div class="copyright-text">
                            <p>Copyright &copy; 2021 Star Cleaning Service</p>
                            <p> Design: Merve Karakaya</p>
                        </div>
                    </div>
                    <div class="col-md-2 col-sm-2 text-align-center">
                        <div class="angle-up-btn">
                            <a href="#top" class="smoothScroll wow fadeInUp" data-wow-delay="1.2s"><i class="fa fa-angle-up"></i></a>
                        </div>
                    </div>
                </div>

            </div>
        </div>
    </footer>

    <!-- SCRIPTS -->
    <script src="~/js/jquery.js"></script>
    <script src="~/js/bootstrap.min.js"></script>
    <script src="~/js/jquery.sticky.js"></script>
    <script src="~/js/jquery.stellar.min.js"></script>
    <script src="~/js/wow.min.js"></script>
    <script src="~/js/smoothscroll.js"></script>
    <script src="~/js/owl.carousel.min.js"></script>
    <script src="~/js/custom.js"></script>

</body>
</html>


